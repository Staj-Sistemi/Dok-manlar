            <div class="col-sm-6">
              <h1>Rapor Güncelleme</h1>
            </div>
          </div>
        </div><!-- /.container-fluid -->
      </section>
      <!-- Main content -->
      <section class="content">
        <div class="container-fluid" style="margin-left: 13%;">
          <div class="row">
            <div class="col-md-9">
              <div class="card card-primary card-outline">
                <div class="card-header">
                  <h3 class="card-title">Günlük Staj Raporu</h3>
                </div>
                <!-- /.card-header -->
                <div class="card-body p-0">
                  <div class="mailbox-read-info">
                    <div class="">
                      <table style="width:100%;">
                        <form action="/student/updateReport/<%=results[0].staj_defteri_ID%>" method="POST">
                        <tr>
                          <td style="width: 400px;">
                            <label for="exampleInputEmail1">Öğrenci Ad Soyad</label>
                            <!-- <input type="text" class="form-control" id="exampleInputEmail1" placeholder="Konuyu giriniz"> -->
                            <div class="text-muted">
                              <%=results[0].ogrenci_ad%> <%=results[0].ogrenci_soyad%>
                            </div>
                          </td>
                          <td style="width: 400px;">
                            <label for="exampleInputPassword1" style="text-align: center;">Firmanın Adı</label>
                            <div class="text-muted">
                              <%=results[0].firma_adi%>
                            </div>
                          </td>
                        </tr>
                      </table>
                      <br>
                    </div>
                  </div>
                  <!-- /.mailbox-read-info -->
                  <div class="mailbox-controls with-border ">
                    <div class="">
                      <table style="width:100%;">
                        <tr>
                          <td style="width: 400px;">
                            <label >Yapılan İş</label>
                            <input type="text" class="form-control" name="yapilan_is" value="<%=results[0].yapilan_is%>"
                              placeholder="<%=results[0].yapilan_is%>">
                          </td>
                          <td style="width: 400px;">
                            <label style="text-align: center;">Tarih</label>
                            <input type="text" class="form-control" name="tarih" value="<%=results[0].tarih%>" placeholder="<%=results[0].tarih%>">
                          </td>
                        </tr>
                      </table>
                      <br>
                    </div>
                  </div>
                  <!-- /.mailbox-controls -->
                  <!-- /.mailbox-read-message -->
                </div>
                <!-- /.card-body -->
                <div class="card-footer bg-white">
                  <div class="form-group">
                    <!-- <label for="inputDescription">Rapor İçeriği</label> -->
                    <textarea id="inputDescription" class="form-control" rows="50" name="aciklama" cols="40"><%=results[0].aciklama%> </textarea>
                  </div>
                </div>
                <!-- /.card-footer -->
                <div class="modal fade" id="modal-primary">
                  <div class="modal-dialog">
                    <div class="modal-content bg-primary">
                      <div class="modal-header">
                        <h4 class="modal-title">Raporunuz Güncellenmiştir!!</h4>
                      </div>
                      <div class="modal-footer justify-content-between">
                        <button style="background-color: white;" type="button" class="btn btn-primary "> <a
                            href="/student/internshipsNotebook">Tamam</a></button>
                      </div>
                    </div>
                    <!-- /.modal-content -->
                  </div>
                  <!-- /.modal-dialog -->
                </div>
                <!-- /.modal -->
                <div class="card-footer">
                  <div class="float-right">
                    <button type="submit" class="btn btn-primary " data-toggle="modal" data-target="#modal-primary"><i
                        class="fas fa-reply"></i> Güncelle</button>
                  </div>
                </div>
              </form>
