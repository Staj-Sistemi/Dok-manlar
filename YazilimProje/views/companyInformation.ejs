    <div class="content-wrapper">
      <!-- Content Header (Page header) -->

      <!-- firma -->
      <div class="card card-primary">
        <div class="card-header">
          <strong class="card-title"><%=results[0].firma_adi%> </strong>
        </div>

        <div class="card-body">
          <p>Lokasyon</p>
          <div class="text-muted">
            <%=results[0].lokasyon%>
          </div>

        </div>
        <div class="card-body">
          <p>Kuruluş Tarihi</p>
          <div class="text-muted">
            <%=results[0].kurulus_tarihi%>
          </div>

        </div>

        <div class="card-body">
          <p>Personel Sayısı</p>
          <div class="text-muted">
            <%=results[0].personel_sayisi%>
          </div>

        </div>

        <div class="card-body">
          <p>Çalışma Alanları</p>
          <div class="text-muted">
            Temelleri 1950 yılında atılan ve otomotiv yan sanayisinin katma değer üreten önemli kuruluşları
            arasında yer alan Coşkunöz Holding’in aynı zamanda savunma, havacılık, enerji, çevre teknolojileri ve
            bilişim alanlarında
            yurt içinde ve yurt dışında yatırımları mevcuttur.

          </div>

        </div>

        <div class="card-body">
          <p>Sosyal Medya</p>
          <div class="text-muted">
            <a href=" https://www.linkedin.com/company/co-kun-z-holding/"> LinkedIn </a> &nbsp;
            <a href="https://www.instagram.com/coskunozholding/?hl=tr"> Instagram </a> &nbsp;
            <a href="https://www.youtube.com/channel/UCh2lrB-VYBdR1ej7Sq1ql9g"> YouTube </a> &nbsp;
            <a href="https://twitter.com/CoskunozHolding"> Twitter </a>
          </div>

        </div>

      </div>
      <div class="modal fade" id="modal-primary">
        <div class="modal-dialog">
          <div class="modal-content bg-primary">
            <div class="modal-header">
              <h4 class="modal-title">İşlemi Onaylıyor Musunuz?</h4>

            </div>

          </div>
          <!-- /.modal-content -->
        </div>
        <!-- /.modal-dialog -->
      </div>
      <!-- /.modal -->
      <!-- Main content -->
      <section class="content">

        <!-- Default box -->
        <div class="card">

          <div class="card-body p-0">
            <table class="table table-striped projects">
              <thead>
                <tr>
                  <th>
                    Çalışma Alanı
                  </th>
                  <th class="text-center">
                    Gereksinimler
                  </th>
                  <th>
                    Tarih Aralığı
                  </th>
                  <th class="text-center">
                    Kontenjan
                  </th>
                  <th>
                  </th>
                </tr>
              </thead>
              <tbody>
                <% result.forEach(result => { %>
                <tr style="height: 100px;">

                  <td>
                    <a>
                      <%=result.calisma_alani%>
                    </a>
                    <br />
                    <small>

                    </small>
                  </td>

                  <td class="project_progress">
                    <%=result.gereksinim%>

                  </td class="project_progress">
                  <td>
                    <%=result.tarih_araligi%>
                  </td>
                  <td class="project-state">
                    <%=result.kontenjan%>
                  </td>

                  <td class="project-actions-right">

                    <button type="button" class="btn btn-info btn-sm" > <a class="btn btn-info btn-sm"
                      href="/student/apply/<%=result.firma_ID%>/<%=result.basvuru_ID%>/<%=ogrenci%>"> <i class="fas fa-check">
                      </i>Başvur</a></button>

                    <!-- <button type="submit" ><a class="btn btn-info btn-sm" href="/student/apply/<%=result.firma_ID%>/<%=result.basvuru_ID%>/<%=ogrenci%>" data-toggle="modal">
                      <i class="fas fa-check">
                      </i>
                      Başvur
                    </a></button> -->

                  </td>
                </tr>
                <% }) %>
                <!-- <div class="modal-footer justify-content-between">
                  <button style="background-color: white;" type="button" class="btn btn-primary "> <a
                      href="../companyInformation/<%=id%>">İptal</a></button>
                  <button style="background-color: white;" type="button" class="btn btn-primary "> <a
                      href="/student/apply/<%=result.firma_ID%>/<%=result.basvuru_ID%>/<%=ogrenci%>">Evet</a></button>
                </div> -->
              </tbody>
            </table>

          </div>

          <!-- /.card-body -->
        </div>
        <!-- /.card -->



